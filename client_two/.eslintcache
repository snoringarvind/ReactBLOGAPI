/ console.log(blogList);\n    if (blogList.length == 0) {\n      return <div>No Blogs. You can add blogs by clicking on create</div>;\n    } else {\n      let arr = [];\n      for (let i = 0; i < blogList.length; i++) {\n        let x = (\n          <div className=\"card\" key={uniqid()}>\n            <div className=\"card-border\">\n              <Link to={`/blog/${blogList[i]._id}`} className=\"card-link\">\n                <div className=\"title\">{blogList[i].title}</div>\n                <div className=\"content list-content\">\n                  {blogList[i].content}\n                </div>\n              </Link>\n            </div>\n          </div>\n        );\n        arr.push(x);\n      }\n      return arr;\n    }\n  };\n\n  const displayError = () => {\n    return <div className=\"error\">{error}</div>;\n  };\n\n  return (\n    <div className=\"List\">\n      {loading && \"loading.....\"}\n      {!loading && (error ? displayError() : displayBlogs())}\n    </div>\n  );\n};\n\nexport default List;\n","/home/arvind/Documents/React-apps/ReactBLOGAPI/client_two/src/Components/Logout.js",[],"/home/arvind/Documents/React-apps/ReactBLOGAPI/client_two/src/Components/Detail/Detail.js",["73","74","75"],"/home/arvind/Documents/React-apps/ReactBLOGAPI/client_two/src/Components/Detail/CommentDisplay.js",["76","77","78","79","80","81"],"/home/arvind/Documents/React-apps/ReactBLOGAPI/client_two/src/Components/Detail/CommentForm.js",["82","83","84","85"],"/home/arvind/Documents/React-apps/ReactBLOGAPI/client_two/src/Components/Navigation/Navigation.js",["86"],"/home/arvind/Documents/React-apps/ReactBLOGAPI/client_two/src/Components/Login/Login.js",["87","88","89","90"],"/home/arvind/Documents/React-apps/ReactBLOGAPI/client_two/src/Components/Signup/Signup.js",["91","92","93"],"import React, { useState } from \"react\";\nimport axios from \"axios\";\nimport \"./Signup.css\";\nimport uniqid from \"uniqid\";\nimport Login from \"../Login/Login\";\nimport { Redirect } from \"