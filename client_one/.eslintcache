oading] = useState(true);\n\n  const axios_blogList = async () => {\n    try {\n      const response = await axios({\n        url: \"http://localhost:3000/api/blogs\",\n        method: \"GET\",\n      });\n      // console.log(response.data);\n      setBlogList(response.data);\n      setLoading(false);\n      setError(\"\");\n    } catch (err) {\n      console.log(\"List=\", err.message);\n      setError(err.message); //important server errors\n      setLoading(false);\n    }\n  };\n\n  useEffect(() => {\n    axios_blogList();\n  }, []);\n\n  const displayBlogs = () => {\n    // console.log(blogList);\n    if (blogList.length == 0) {\n      return <div>No Blogs. You can add blogs by clicking on create</div>;\n    } else {\n      let arr = [];\n      for (let i = 0; i < blogList.length; i++) {\n        let x = (\n          <div className=\"card\" key={uniqid()}>\n            <div className=\"card-border\">\n              <Link to={`/blog/${blogList[i]._id}`} className=\"card-link\">\n                <div className=\"title\">{blogList[i].title}</div>\n                <div className=\"content list-content\">\n                  {blogList[i].content}\n                </div>\n              </Link>\n            </div>\n          </div>\n        );\n        arr.push(x);\n      }\n      return arr;\n    }\n  };\n\n  const displayError = () => {\n    return <div className=\"error\">{error}</div>;\n  };\n\n  return (\n    <div className=\"List\">\n      {loading && \"loading.....\"}\n      {!loading && (error ? displayError() : displayBlogs())}\n    </div>\n  );\n};\n\nexport default List;\n","/home/arvind/Documents/React-apps/ReactBLOGAPI/client_one/src/Components/AuthButton.js",[],"/home/arvind/Documents/React-apps/ReactBLOGAPI/client_one/src/Components/Logout.js",[],"/home/arvind/Documents/React-apps/ReactBLOGAPI/client_one/src/Components/Update.js",["77"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { useParams } from \"react